<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>privileges</filter-name>
    <filter-class>cn.dhr.web.filter.PrivilegesFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>privileges</filter-name>
    <url-pattern>/jsp/order_list.jsp</url-pattern>
    <url-pattern>/jsp/order_info.jsp</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>Encoding</filter-name>
    <filter-class>cn.dhr.web.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>Encoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>